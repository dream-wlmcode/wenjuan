.table a.a_to_breadcrumb{
	cursor: pointer;
	color: #9c27b0;
}
.table .tbmr{
	cursor: pointer;
	margin-right: 10px;
}
.table .tbmr:nth-last-child(1){
	margin-right: 0;
}
.table .rowdelete{
	color:#ef5350;
}
.table .rowdeleteUR{
	color:#ef5350;
}
.table .rowsetdefault.default{
	color:#42a5f5;
}
.table .rowkeyis{
	font-size: 16px;
	font-weight: bold;
	margin-right: 5px;
	color: #ffca28;
}
.form-check-hllist.form-check{
	padding-left: 0;
}
.form-check-hllist.form-check .checkbox-custom{
	left: 0;
	top: 0;
}



/*个人中心 s*/
.perCenter-content .tab-content.none-mtbr-tabc{
	margin-top: 0;
	border:none;
}
.nav-pills .nav-link {
    border-radius: 2px;
    color: #42a5f5;
}
.nav-pills.custom-pills .nav-link {
    border-radius: 0;
    opacity: .7;
}
.nav-pills .nav-link.active, 
.nav-pills .show>.nav-link {
    color: #fff;
    background-color: #42a5f5;
}
.nav-pills.custom-pills .nav-link.active {
    color: #42a5f5;
    opacity: 1;
    background-color: transparent;
    border-bottom: 2px solid #42a5f5;
}
